  "description": "Plant Tracker",
  "devDependencies": {
    "babel-eslint": "8.2.5",
    "check-node-version": "3.2.0",
    "concurrently": "3.6.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jest": "21.17.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.10.0",
    "eslint-plugin-security": "1.4.0",
    "jest": "23.2.0",
    "nodemon": "1.17.5",
    "npm-update-outdated": "0.1.6",
    "pre-commit": "1.2.2",
    "react-hot-loader": "4.3.3",
    "react-log-lifecycle": "1.1.0",
    "react-test-renderer": "16.4.1",
    "redux-devtools": "3.4.1",
    "stylelint": "9.3.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-order": "^0.8.0",
    "webpack-dev-server": "2.x"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "jestConfig": "./jest.config.js",
  "homepage": "https://github.com/guyellis/plant",
  "keywords": [
    "backyard",
    "orchard",
    "plant",
    "tracker"
  ],
  "license": "MIT",
  "main": "server.js",
  "name": "plant",
  "pre-commit": {
    "colors": true,
    "run": [
      "precom"
    ],
    "silent": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guyellis/plant.git"
  },
  "scripts": {
    "build": "TARGET=build webpack --bail",
    "cnv": "check-node-version --package",
    "cover": "jest --coverage && google-chrome coverage/lcov-report/index.html",
    "coverage": "jest --forceExit --coverage",
    "coverage-fast": "jest --forceExit --coverage --onlyChanged",
    "coverage-view": "google-chrome coverage/lcov-report/index.html",
    "csslint": "stylelint app/stylesheets/*.css",
    "dependency-update": "node_modules/.bin/npm-update-outdated",
    "lint": "eslint --ext .js --ext .jsx . --max-warnings 0",
    "lintfix": "eslint --ext .js --ext .jsx . --fix",
    "precom": "concurrently --kill-others-on-fail \"npm:cnv\" \"npm:lint\" \"npm:csslint\" \"npm:build\" \"./devops/run-test.sh\"",
    "prodstart": "npm run build && pm2 start ../site-ops/pm2/plant.config.js --env production",
    "prodstop": "pm2 stop plant",
    "server": ". ./devops/run-server.sh",
    "start": "TARGET=dev webpack-dev-server --port 9090 --progress --colors --hot --inline --history-api-fallback --bail",
    "test": "npm run lint && npm run build && ./devops/run-test.sh",
    "test-only": "jest --forceExit",
    "test-fast": "jest --forceExit --onlyChanged"
  },
  "version": "0.0.1"
}
