  "main": "server.js",
  "name": "plant",
  "pre-commit": {
    "colors": true,
    "run": [
      "csslint",
      "test"
    ],
    "silent": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guyellis/plant.git"
  },
  "scripts": {
    "build": "TARGET=build webpack --bail",
    "coverage": "jest -w 1 --forceExit --coverage",
    "csslint": "stylelint app/stylesheets/*.css",
    "dependency-update": "node_modules/.bin/npm-update-outdated",
    "lint": "eslint --ext .js --ext .jsx . --max-warnings 0",
    "lintfix": "eslint --ext .js --ext .jsx . --fix",
    "prepublish": "npm run build",
    "prodstart": "npm run build && pm2 start ../site-ops/pm2/plant.config.js --env production",
    "prodstop": "pm2 stop plant",
    "server": ". ./devops/run-server.sh",
    "start": "TARGET=dev webpack-dev-server --progress --colors --hot --inline --history-api-fallback --bail",
    "test": "npm run lint && npm run build && sh ./devops/run-test.sh",
    "test-only": "jest -w 1 --forceExit",
    "test-fast": "jest -w 1 --forceExit --onlyChanged"
  },
  "version": "0.0.1"
}
