{
  "lines": 20,
  "statements": 20,
  "functions": 20,
  "branches":20,
  "exclude": [
    "server.js",
    "webpack.config.js",
    "devops/**",
    "node_modules/**",
    "test/**",
    ".vscode/**",
    "build/**"
  ],
  "reporter": [
    "lcov",
    "text-summary"
  ],
  "extension": [
    ".js",
    ".jsx"
  ],
  "cache": true,
  "all": true,
  "check-coverage": true
}
